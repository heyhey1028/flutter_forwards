{
  "name": "オリジナル入力チェック",
  "version": "1.0",
  "description": "必須チェックを追加する。",
  "content_scripts": [
    {
      "matches": ["https://app.abceed.com/*"],
      "js": ["abceed.js"]
    },
    {
      "matches": ["https://nativecamp.net/*"],
      "js": ["native_camp.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "https://app.abceed.com/analysis/timeBook/ask_deru1000",
    "https://nativecamp.net/lesson-history"
  ],
  "manifest_version": 3
}
